{
  "name": "@my-ui/core",
  "version": "2.8.0",
  "scripts": {
    "start": "webpack serve --env development",
    "start-with-storybook": "yarn start && yarn storybook",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "build-npm": "webpack --mode=production --config webpack.config.npm.js && tsc --project tsconfig.npm.json && tscpaths -p tsconfig.npm.json -s ./src -o ./dist",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "yarn pretty-quick",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "start-app": "docker-compose up --build",
    "kill-app": "docker-compose down",
    "pre-commit": "yarn format && git add .",
    "pre-push": "yarn format"
  },
